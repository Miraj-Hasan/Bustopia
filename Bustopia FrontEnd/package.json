{
  "name": "bustopiafrontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "cypress:run:chrome": "cypress run --browser chrome",
    "cypress:run:headless": "cypress run --headless",
    "test:e2e": "cypress run --headless",
    "test:e2e:dev": "cypress open",
    "test:ci": "cypress run --browser chrome --headless --reporter spec",
    "test:staging": "CYPRESS_BASE_URL=https://your-staging-frontend.com CYPRESS_API_BASE_URL=https://your-staging-backend.com cypress run --headless",
    "test:production": "CYPRESS_BASE_URL=https://your-production-frontend.com CYPRESS_API_BASE_URL=https://your-production-backend.com cypress run --headless",
    "test:staging:open": "CYPRESS_BASE_URL=https://your-staging-frontend.com CYPRESS_API_BASE_URL=https://your-production-backend.com cypress open",
    "test:production:open": "CYPRESS_BASE_URL=https://your-production-frontend.com CYPRESS_API_BASE_URL=https://your-production-backend.com cypress open",
    "test": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage --reporter=verbose",
    "test:watch": "vitest --watch",
    "test:ci:unit": "vitest run --coverage --reporter=verbose --run"
  },
  "dependencies": {
    "axios": "^1.9.0",
    "bootstrap": "^5.3.6",
    "bootstrap-icons": "^1.13.1",
    "date-fns": "^4.1.0",
    "react": "^18.2.0",
    "react-bootstrap": "^2.10.10",
    "react-chartjs-2": "^5.3.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^7.6.0",
    "react-toastify": "^11.0.5",
    "socket.io-client": "^4.8.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.25.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/jsdom": "^21.1.7",
    "@types/react": "^19.1.2",
    "@types/react-dom": "^19.1.2",
    "@vitejs/plugin-react": "^4.4.1",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "cypress": "^14.5.1",
    "eslint": "^9.25.0",
    "eslint-plugin-cypress": "^5.1.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "globals": "^16.0.0",
    "happy-dom": "^18.0.1",
    "jsdom": "^21.1.0",
    "typescript": "^5.8.3",
    "vite": "^6.3.5",
    "vite-plugin-mkcert": "^1.17.8",
    "vitest": "^3.2.4",
    "vitest-canvas-mock": "^0.3.3"
  }
